                      //上传图片
                      uploadImg($("#uploadBtn")[0].files[0], 4, liveid);
                    });

                  break;
                }
                  if($(".mh").length==4){
                    $("#add").hide();
                  }
                  layer.close(index);
                }
              })       
            });
      });
